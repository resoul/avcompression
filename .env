# Application
APP_ENV=development
APP_LOG_LEVEL=debug
APP_WORKER_ID=worker-local-1
APP_TIMEOUT=5m
APP_MAX_RETRIES=3

# MinIO
MINIO_ENDPOINT=localhost:9000
MINIO_ACCESS_KEY=airlance
MINIO_SECRET_KEY=airlance
MINIO_SECURE=false

# RabbitMQ
RABBITMQ_URL=amqp://airlance:airlance@localhost:5672/
RABBITMQ_QUEUE=jobs

# Metrics
METRICS_ENABLED=true
METRICS_PORT=2112
METRICS_PATH=/metrics